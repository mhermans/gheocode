     "output_type": "stream",
     "text": [
      "Acacialaan 9080 Lochristi\n",
     ]
    }
   ],
   "source": [
    "responses = []\n",
    "for address in addressen['full_address']:\n",
    "    print(address)\n",
    "    data = get_address_info(address)\n",
    "    responses.append(data)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "metadata": {
    "collapsed": false,
    "deletable": true,
    "editable": true
   },
   "outputs": [],
   "source": [
